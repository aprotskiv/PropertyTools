<Window x:Class="PropertyGridDemos.GettingStartedExample"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:p="clr-namespace:PropertyTools.Wpf;assembly=PropertyTools.Wpf"
        xmlns:d="clr-namespace:PropertyGridDemos"
        Title="1 Getting started" Height="450" Width="900">
    <Window.DataContext>
        <d:GettingStartedExampleViewModel/>
    </Window.DataContext>
    <Grid Margin="4">
        <p:PropertyGrid SelectedObject="{Binding SelectedObject}">
        </p:PropertyGrid>
    </Grid>
</Window>
